{
    "version": 2,
    "builds": [
        {
            "src": "server/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "client/dist/index.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/client/dist/index.html"
        }
    ]
}